                   RowBox[{"{", 
                    RowBox[{"dir", ",", "\"\<cmake\>\""}], "}"}]}]}], "\n", 
                 "\t", "\t", "\t\t\t\t\t", "]"}], ";", "\n", "\t", 
                "\t\t\t\t\t", 
                RowBox[{"DeleteDirectory", "[", 
                 RowBox[{"d", ",", 
                  RowBox[{"DeleteContents", "\[Rule]", "True"}]}], "]"}]}]}], 
              "\n", "\t\t\t\t\t\t", "]"}], ",", "\n", "\t\t\t\t\t", 
