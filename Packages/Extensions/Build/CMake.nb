                  RowBox[{"FileNameJoin", "@", 
                   RowBox[{"{", 
                    RowBox[{
                    "$TemporaryDirectory", ",", "$cmakeInstallation"}], 
                    "}"}]}], ",", "\n", "\t", "\t\t\t\t\t\t", "d"}], "\n", 
                 "\t", "\t\t\t\t\t\t", "]"}], ";", "\n", "\t", "\t\t\t\t\t", 
