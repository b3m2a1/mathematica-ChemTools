                  RowBox[{"FileNameJoin", "@", 
                   RowBox[{"{", "\n", "\t", "\t\t\t\t\t\t\t", 
                    RowBox[{"d", ",", "\n", "\t", "\t\t\t\t\t\t\t", 
                    RowBox[{"StringTrim", "[", 
                    RowBox[{"$cmakeInstallation", ",", "\"\<.tar.gz\>\""}], 
