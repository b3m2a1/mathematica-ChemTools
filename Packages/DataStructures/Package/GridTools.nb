Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"iGridPermute", "[", 
    RowBox[{"g_List", ",", " ", "newIndices_"}], "]"}], ":=", "\n", "\t", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", "newGrid", "}"}], ",", "\n", "\t", "\t", 
     RowBox[{
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"GridDimension", "@", "g"}], "=!=", 
         RowBox[{"Length", "@", "newIndices"}]}], ",", "\n", "\t", "\t\t", 
        RowBox[{"PackageRaiseException", "[", "\n", "\t", "\t\t\t", 
         RowBox[{
         "\"\<Can't perform permutation `` on grid of dimension ``\>\"", ",", 
          "\n", "\t", "\t\t\t", "newIndices", ",", "\n", "\t", "\t\t\t", 
          RowBox[{"GridDimension", "@", "g"}]}], "\n", "\t\t\t\t", "]"}]}], 
       "\n", "\t\t\t", "]"}], ";", "\n", "\t\t", 
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"Sort", "@", "newIndices"}], "=!=", 
         RowBox[{"Range", "[", 
          RowBox[{"Length", "@", "newIndices"}], "]"}]}], ",", "\n", "\t\t\t", 
        RowBox[{"PackageRaiseException", "[", "\n", "\t\t\t\t", 
         RowBox[{
         "\"\<Permutation `` loses coordinates ``\>\"", ",", "\n", "\t\t\t\t",
           "newIndices", ",", "\n", "\t\t\t\t", 
          RowBox[{"Sort", "@", "\n", "\t\t\t\t\t", 
           RowBox[{"Complement", "[", 
            RowBox[{
             RowBox[{"Range", "[", 
              RowBox[{"Length", "@", "newIndices"}], "]"}], ",", " ", 
             "newIndices"}], "]"}]}]}], "\n", "\t\t\t\t", "]"}]}], "\n", 
       "\t\t\t", "]"}], ";", "\n", "\t\t", 
      RowBox[{"newGrid", "=", 
       RowBox[{"Transpose", "[", 
        RowBox[{"g", ",", " ", "newIndices"}], "]"}]}], ";", "\n", "\t\t", 
      RowBox[{"Part", "[", 
       RowBox[{"newGrid", ",", "\n", "\t\t\t", 
        RowBox[{"Sequence", "@@", "\n", "\t\t\t\t", 
         RowBox[{"Append", "[", "\n", "\t\t\t\t\t", 
          RowBox[{
           RowBox[{"ConstantArray", "[", 
            RowBox[{"All", ",", " ", 
             RowBox[{
