        "\n", "\t\t\t", 
        RowBox[{"vDiag", "=", 
         RowBox[{"Rescale", "[", "vDiag", "]"}]}], ";", "\n", "\t\t\t", 
        RowBox[{"Pick", "[", 
         RowBox[{"gridpoints", ",", " ", 
          RowBox[{
           RowBox[{
            RowBox[{"#", "<=", "prune"}], "&"}], "/@", "vDiag"}]}], "]"}]}], 
       ",", "\n", "\t\t\t", "gridpoints"}], "\n", "\t\t\t", "]"}]}], "\n", 
