\"", ",", "\n", "\t\t\t\t\t", 
           RowBox[{"Dimensions", "[", "grid", "]"}], ",", "\n", "\t\t\t\t\t", 
           
           RowBox[{"Dimensions", "[", "base", "]"}]}], "\n", "\t", "\t\t\t\t",
           "]"}]}], "\n", "\t\t\t\t", "]"}]}], "\n", "\t\t\t", "]"}], ";", 
     "\n", "\t\t", 
     RowBox[{"{", "\n", "\t\t\t", 
