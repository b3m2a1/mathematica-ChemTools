Cell[BoxData[
 RowBox[{
  RowBox[{"cleanGridData", "[", "g_", "]"}], ":=", "\n", "\t", 
  RowBox[{"With", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"base", "=", 
      RowBox[{"Developer`ToPackedArray", "@", 
       RowBox[{"N", "@", "g"}]}]}], "}"}], ",", "\n", "\t\t", 
    RowBox[{
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"!", 
        RowBox[{"Developer`PackedArrayQ", "[", "base", "]"}]}], ",", "\n", 
