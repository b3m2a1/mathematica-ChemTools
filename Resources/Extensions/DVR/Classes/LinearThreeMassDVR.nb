Cell[BoxData[
 RowBox[{
  RowBox[{"LinearThreeMass2DDVRKineticMatrix", "[", 
   RowBox[{"grid_", ",", " ", "\n", "\t", 
    RowBox[{"ops", ":", 
     RowBox[{"OptionsPattern", "@", 
      RowBox[{"{", "\n", "\t\t", 
       RowBox[{
        RowBox[{"\"\<m1\>\"", "->", "1"}], ",", "\n", "\t\t", 
        RowBox[{"\"\<m2\>\"", "\[Rule]", "1"}], ",", "\n", "\t\t", 
        RowBox[{"\"\<m3\>\"", "\[Rule]", "1"}], ",", "\n", "\t\t", 
        RowBox[{"\"\<\[HBar]\>\"", "->", "1"}]}], "\n", "\t\t", "}"}]}]}]}], 
   "\n", "\t", "]"}], ":=", "\n", "\t", 
  RowBox[{"Module", "[", "\n", "\t\t", 
   RowBox[{
    RowBox[{"{", "\n", "\t\t\t", 
     RowBox[{
      RowBox[{"cartDVR1", "=", "\n", "\t\t\t\t", 
       RowBox[{"Cartesian1DDVRKineticMatrix", "[", "\n", "\t\t\t\t\t", 
        RowBox[{
         RowBox[{"grid", "[", 
          RowBox[{"[", 
           RowBox[{"All", ",", " ", "1", ",", " ", "1"}], "]"}], "]"}], ",", 
         " ", "\n", "\t\t\t\t\t", 
         RowBox[{"FilterRules", "[", 
          RowBox[{
           RowBox[{"{", "\n", "\t\t\t\t\t\t", 
            RowBox[{
             RowBox[{"\"\<M\>\"", "\[Rule]", "1"}], ",", "\n", "\t\t\t\t\t\t",
              "ops"}], "\n", "\t\t\t\t\t\t", "}"}], ",", "\n", "\t\t\t\t\t\t", 
           RowBox[{"Options", "[", "Cartesian1DDVRKineticMatrix", "]"}]}], 
          "\n", "\t\t\t\t\t\t", "]"}]}], "\n", "\t\t\t\t\t", "]"}]}], ",", 
      "\n", "\t\t\t", 
      RowBox[{"cartDVR2", "=", "\n", "\t\t\t\t", 
       RowBox[{"Cartesian1DDVRKineticMatrix", "[", "\n", "\t\t\t\t\t", 
        RowBox[{
         RowBox[{"grid", "[", 
          RowBox[{"[", 
           RowBox[{"1", ",", " ", "All", ",", " ", "2"}], "]"}], "]"}], ",", 
         "\n", "\t\t\t\t\t", 
         RowBox[{"FilterRules", "[", 
          RowBox[{
           RowBox[{"{", "\n", "\t\t\t\t\t\t", 
            RowBox[{
             RowBox[{"\"\<M\>\"", "\[Rule]", "1"}], ",", "\n", "\t\t\t\t\t\t",
              "ops"}], "\n", "\t\t\t\t\t\t", "}"}], ",", "\n", "\t\t\t\t\t\t", 
           RowBox[{"Options", "[", "Cartesian1DDVRKineticMatrix", "]"}]}], 
          "\n", "\t\t\t\t\t\t", "]"}]}], "\n", "\t\t\t\t\t", "]"}]}], ",", 
      "\n", "\t\t\t", 
      RowBox[{"ptsX", "=", 
       RowBox[{"Length", "@", "grid"}]}], ",", "\n", "\t\t\t", 
      RowBox[{"ptsY", "=", 
       RowBox[{"Length", "@", 
        RowBox[{"grid", "[", 
         RowBox[{"[", "1", "]"}], "]"}]}]}], ",", "\n", "\t\t\t", 
      RowBox[{"m1", "=", 
       RowBox[{"OptionValue", "[", "\"\<m1\>\"", "]"}]}], ",", "\n", "\t\t\t", 
      RowBox[{"m2", "=", 
       RowBox[{"OptionValue", "[", "\"\<m2\>\"", "]"}]}], ",", "\n", "\t\t\t", 
      RowBox[{"m3", "=", 
       RowBox[{"OptionValue", "[", "\"\<m3\>\"", "]"}]}]}], "\n", "\t\t\t", 
     "}"}], ",", "\n", "\t\t", 
    RowBox[{
     RowBox[{"cartDVR1", "=", "\n", "\t\t\t", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"1", "/", "m1"}], ")"}], "*", "cartDVR1"}]}], ";", "\n", 
     "\t\t", 
     RowBox[{"cartDVR2", "=", "\n", "\t\t\t", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         RowBox[{"1", "/", "m3"}], "+", 
         RowBox[{"2", "/", "m2"}]}], ")"}], "*", "cartDVR2"}]}], ";", "\n", 
     "\t\t", 
     RowBox[{
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"(", 
          RowBox[{"ptsX", "*", "ptsY"}], ")"}], ">", "100000"}], ",", "\n", 
        "\t\t\t", "ParallelTable", ",", "\n", "\t\t\t", "Table"}], "\n", 
       "\t\t\t", "]"}], "[", "\n", "\t\t\t", 
      RowBox[{
       RowBox[{"With", "[", "\n", "\t\t\t\t", 
        RowBox[{
         RowBox[{"{", "\n", "\t\t\t\t\t", 
          RowBox[{
           RowBox[{"ix", "=", 
            RowBox[{"1", "+", 
             RowBox[{"Floor", "[", 
              RowBox[{
               RowBox[{"(", 
                RowBox[{"i", "-", "1"}], ")"}], "/", 
               RowBox[{"(", "ptsY", ")"}]}], "]"}]}]}], ",", " ", 
           RowBox[{"jx", "=", 
            RowBox[{"1", "+", 
             RowBox[{"Floor", "[", 
              RowBox[{
               RowBox[{"(", 
                RowBox[{"j", "-", "1"}], ")"}], "/", 
               RowBox[{"(", "ptsY", ")"}]}], "]"}]}]}], ",", "\n", 
           "\t\t\t\t\t", 
           RowBox[{"iy", "=", 
            RowBox[{"Mod", "[", 
             RowBox[{"i", ",", " ", "ptsY", ",", " ", "1"}], "]"}]}], ",", 
           " ", 
           RowBox[{"jy", "=", 
            RowBox[{"Mod", "[", 
             RowBox[{"j", ",", " ", "ptsY", ",", " ", "1"}], "]"}]}]}], "\n", 
          "\t\t\t\t\t", "}"}], ",", "\n", "\t\t\t\t", 
         RowBox[{
          RowBox[{"If", "[", 
           RowBox[{
            RowBox[{"iy", "\[Equal]", "jy"}], ",", "\n", "\t\t\t\t\t\t", "0", 
            ",", "\n", "\t\t\t\t\t\t", 
            RowBox[{"cartDVR1", "[", 
             RowBox[{"[", 
              RowBox[{"ix", ",", " ", "jx"}], "]"}], "]"}]}], "\n", 
           "\t\t\t\t\t\t", "]"}], "+", "\n", "\t\t\t\t", 
          RowBox[{"If", "[", 
           RowBox[{
            RowBox[{"ix", "\[Equal]", "jx"}], ",", "\n", "\t\t\t\t\t\t", "0", 
            ",", "\n", "\t\t\t\t\t\t", 
            RowBox[{"cartDVR2", "[", 
             RowBox[{"[", 
              RowBox[{"iy", ",", " ", "jy"}], "]"}], "]"}]}], "\n", 
           "\t\t\t\t\t\t", "]"}]}]}], "\n", "\t\t\t\t", "]"}], ",", "\n", 
       "\t\t\t", 
       RowBox[{"{", 
        RowBox[{"i", ",", " ", 
         RowBox[{"ptsX", "*", "ptsY"}]}], "}"}], ",", "\n", "\t\t\t", 
       RowBox[{"{", 
        RowBox[{"j", ",", " ", 
         RowBox[{"ptsX", "*", "ptsY"}]}], "}"}]}], "\n", "\t\t\t", "]"}]}]}], 
   "\n", "\t\t", "]"}]}]], "CodeInput",
 CellChangeTimes->{{3.699200204109901*^9, 3.6992002446128483`*^9}, {
